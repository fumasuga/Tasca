{
  "expo": {
    "name": "Taskly",
    "slug": "taskly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D12"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fumasuga.taskly.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false
        },
        "UIBackgroundModes": []
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0D12"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fumasuga.taskly.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "taskly",
    "owner": "fumasuga",
    "extra": {
      "eas": {
        "projectId": "1121f368-c4f4-4a75-8ce0-64753828233a"
      }
    }
  }
}
